data Bool = `false | `true

not : (x : Bool)-> Bool =
  \ (x : Bool)-> case x of {
    `false -> `true ,
    `true -> `false
  }

pnot =
  proc (c : ?Bool.!Bool)
  recv c (x : Bool)
  send c (case x of {
    `false -> `true ,
    `true -> `false
  } )

if : (b : Bool)(A : Type)(t : A)(e : A)-> A =
  \ (b : Bool)(A : Type)(t : A)(e : A)-> case b of {
    `true -> t ,
    `false -> e
  }

If : (b : Bool)(A : Type)(B : Type)(t : A)(e : B)-> case b of {
    `true -> A ,
    `false -> B
  } =
  \ (b : Bool)(A : Type)(B : Type)(t : A)(e : B)-> case b of {
    `true -> t ,
    `false -> e
  }

