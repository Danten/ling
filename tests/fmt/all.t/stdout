data T = `a | `b | `c

rot : (x : T)-> T =
  \ (x : T)-> case x of {
    `a -> `b ,
    `b -> `c ,
    `c -> `a
  }

rot2 : (x : T)-> T =
  \ (x : T)-> rot (rot x)

showMult =
  \ (m : Int)(n : Int)-> (showInt m)++S " * " ++S (showInt n)++S " = " ++S showInt (m * n)

showDiv =
  \ (m : Double)(n : Double)-> (showDouble m)++S " / " ++S (showDouble n)++S " = " ++S showDouble (m /D n)

